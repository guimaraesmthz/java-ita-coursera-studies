import java.util.Objects;

public class Produto {

	private String nome;
	private String codigo;
	private double preco;
	
	Produto(String nome, String codigo, double preco) {
		this.nome = nome;
		this.codigo = codigo;
		this.preco = preco;
	};

	
	@Override
	public int hashCode() {
		return Objects.hash(codigo);
	}
	
}
